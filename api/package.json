{
    "name": "borsajs-demo-api",
    "version": "1.0.0",
    "description": "Firebase Functions API for borsajs demo",
    "main": "lib/index.js",
    "type": "module",
    "engines": {
        "node": "20"
    },
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log"
    },
    "dependencies": {
        "borsajs": "^0.2.2",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^5.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "typescript": "^5.3.3"
    },
    "private": true
}